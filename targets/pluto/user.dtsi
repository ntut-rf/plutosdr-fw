/ {
    axidma_chrdev: axidma_chrdev@0 {
        compatible = "xlnx,axidma-chrdev";
        dmas = <&axi_dma_0 0 &axi_dma_0 1>;
        dma-names = "tx_channel", "rx_channel";
    };
};

&primary_flash {
		partition@qspi-linux {
			label = "qspi-linux";
			reg = <0x200000 0x1900000>; /* 25M */
		};
        partition@qspi-user {
			label = "qspi-user";
			reg = <0x1b00000 0x500000>; /* 5M */
		};
	};														